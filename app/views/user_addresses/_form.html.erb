<%= form_for [@user, @address], url: @form_url, method: @method do |f| %>

  <%= f.label :nickname %>
  <%= f.text_field :nickname %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <% f.label :active %>
  <% f.text_field :active %>

  <% f.label :default %>
  <% f.text_field :default %>

  <%= f.submit %>
<% end %>
